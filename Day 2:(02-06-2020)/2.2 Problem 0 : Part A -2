//Fluffy name is spelled wrongly. Update it to Fluffyy
function spell()
{
var cat = {
 name: "Fluffy",
 activities: ["play", "eat cat food"],
 catFriends: [
 {
 name: "bar",
 activities: ["be grumpy", "eat bread omblet"],
 weight: 8,
 furcolor: "white"
 }, 
 {
 name: "foo",
 activities: ["sleep", "pre-sleep naps"],
 weight: 3
 }
 ]
}
// Update it to Fluffyy
cat.name="fluffyy";
console.log(cat);
}
 spell();
